package de.ostfalia.prog.ss22;
/**
 * Figur class
 * Jeder Spieler hat 2 Figuren
 * @author abdel
 *
 */
public class Figur {
	private int figurposition;
	/**
	 * Konstruktor 
	 * @param figurposition
	 */
	public Figur(int figurposition) {
		this.figurposition = figurposition;

	}
	/**
	 * get Methode
	 * @return liefert die position einer Figur
	 */
	public int getFigurposition() {
		return figurposition;
	}
	/**
	 * set Methode
	 * @param figurposition
	 */
	public void setFigurposition(int figurposition) {
		this.figurposition = figurposition;
	}
	/**
	 * toString Methode um Ausgabe zu erm√∂glichen
	 */
	@Override
	public String toString() {
		return String.valueOf(figurposition);
	}

}
